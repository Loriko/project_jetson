@model BackEndServer.Models.ViewModels.LocationInformationList
@{
    ViewBag.Title = "Project Jetson - Location Selection";
}

<h1 id="locationSelectionText" class="page-title center-align">To view information about a camera, please first select the location where the camera is located</h1>
<div id="locations" class="collection">
    @foreach (var location in Model.LocationList)
    {
        <a href="LocationSelected?locationId=@location.LocationId" class="collection-item" id=@location.LocationId>@location.LocationName</a>
    }
</div>