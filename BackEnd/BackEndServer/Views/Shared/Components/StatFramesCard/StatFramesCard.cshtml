@using BackEndServer.Models.ViewModels
@model JpgStatFrameList
<div class="card blue-grey darken-1 full-width" id="stat_frames_card">
    <div class="card-content white-text">
        <span class="card-title">Key Frames</span>
        <div class="carousel">
            @foreach (var jpgFramePath in Model.JpgFramePathList)
            {
                <li class="carousel-item"><img src="../@jpgFramePath.Replace("wwwroot/","")"></li>
            }
        </div>
    </div>
</div>

<script>
    $(document).ready(function(){
        $('.carousel').carousel({});
    });
</script>